{
  "name": "teamnl-cloud9-backend",
  "version": "0.1.1",
  "description": "Clean MVP backend: 6 API endpoints + Supabase sync",
  "type": "module",
  "private": true,
  "main": "src/server.ts",
  "scripts": {
    "start": "npx tsx src/server.ts",
    "dev": "nodemon --watch src --exec npx tsx src/server.ts",
    "build": "cd frontend && npm install && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "sync:event-signups": "npx tsx ../scripts/sync-event-signups.ts",
    "import:events": "npx tsx ../scripts/import-events.ts",
    "sync:live": "npx tsx ../scripts/sync-live.ts"
  },
  "keywords": [
    "zwift",
    "racing",
    "api",
    "supabase"
  ],
  "author": "TeamNL Cloud9",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "@types/node-cron": "^3.0.11",
    "axios": "^1.6.2",
    "axios-cookiejar-support": "^6.0.5",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "node-cron": "^4.2.1",
    "tough-cookie": "^6.0.0",
    "tsx": "^4.20.6"
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  }
}
